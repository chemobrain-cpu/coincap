{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#1652f0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-xxxxxx"
      },
      "package": "com.chemobrain857380.myapp"
    },
    "web": {

    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff"
          
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "20a49b61-dcc3-4c5a-a638-41dcc0f472de"
      }
    }
  }
}

